{
  "name": "seniore",
  "version": "1.0.3",
  "description": "Gracefully manage your GitHub and Jira workflow",
  "author": "Pablo Molina <contact@pablomolina.me>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/p2kmgcl/seniore"
  },
  "main": "build/index.js",
  "files": [
    "package.json",
    "bin/seniore",
    "build/**/*"
  ],
  "bin": {
    "seniore": "bin/seniore",
    "s": "bin/seniore"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "check-format": "prettier --list-different .",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --fix src/**/*.{ts,tsx}"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.ts": "eslint --max-warnings 0 --fix",
    "*.tsx": "eslint --max-warnings 0 --fix"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@octokit/rest": "^17.8.0",
    "commander": "^5.1.0",
    "date-fns": "^2.13.0",
    "execa": "^4.0.0",
    "husky": "^4.2.5",
    "ink": "^2.7.1",
    "jira-client": "^6.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^8.10.60",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.20.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  }
}
